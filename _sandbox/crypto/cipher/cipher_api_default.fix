module Minilib.Crypto.Cipher.CipherApiDefault;

import Minilib.Crypto.Cipher.Rsa.RsaApiDefault;
import Minilib.Crypto.Cipher.Ec.EcApiDefault;
import Minilib.Monad.Random;
import Minilib.Monad.Error;

make: [m: MonadError, m: MonadRandom] m (CipherApi m);
make = (
    pure $ CipherApi {
        rsa_api: some $ *RsaApiDefault::make,
        ec_api: some $ *EcApiDefault::make,
    }
);

