module Main;

import Minilib.Trait.Monoid;
import Minilib.Functional.Endo;

test1: IO ();
test1 = (
    let e = Iterator::range(0, 10).fold(
        mempty, |e, i|
        e.mconcat(endo(add(i.to_string+",")))
    );
    println(e.appEndo("!"))
);

id: a -> a;
id = |x| x;

test2: IO ();
test2 = (
    let f = Iterator::range(0, 10).fold(
        id, |f, i|
        f << add(i.to_string + ",")
    );
    println(f("!"))
);

main: IO ();
main = test1;

