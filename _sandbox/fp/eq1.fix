module Minilib.Trait.Eq1;

trait [f: *->*] f: Eq1 {
    eq1: [a: Eq] f a -> f a -> Bool;
}

impl Array: Eq1 {
    eq1 = eq;
}

impl Option: Eq1 {
    eq1 = eq;
}

impl [e: Eq] Result e: Eq1 {
    eq1 = eq;
}

impl [e: Eq] Tuple2 e: Eq1 {
    eq1 = eq;
}

impl Identity: Eq1 {
    eq1 = |ia1, ia2| ia1.@data == ia2.@data;
}

impl [c: Eq] Const c: Eq1 {
    eq1 = |ca1, ca2| ca1.@data == ca2.@data;
}