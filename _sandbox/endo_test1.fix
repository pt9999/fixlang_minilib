module Main;

import Minilib.Trait.Monoid;
import Minilib.Data.Monoid.Endo;

test1: IO ();
test1 = (
    let e = Iterator::range(0, 10).map(endo << |i| add(i.to_string+",")).mconcat;
    println(e.appEndo("!"))
);

id: a -> a;
id = |x| x;

test2: IO ();
test2 = (
    let f = Iterator::range(0, 10).fold(
        id, |f, i|
        f << add(i.to_string + ",")
    );
    println(f("!"))
);

main: IO ();
main = test1;

