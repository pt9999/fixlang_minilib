        pure $ ans.get_first
    }).lift;
    let (size, size_time) = *consumed_time_while_io(do {
        pure();;
        pure $ ans.fold(0, |i, _| i + 1)
    }).lift;
    println("shuffled, create =" + (shuffled_time, create_time).to_string).lift;;
    println("ans, first, size, total=" + (ans_time, first_time, size_time, ans_time + first_time + size_time).to_string).lift;;
    pure()
);

