test_karatsuba: IO ();
test_karatsuba = (
    let _ = *pure();
    let _ = FFI_CALL[() setbuf(Ptr, Ptr), IO::stdout._file_ptr, nullptr];
    let time = FFI_CALL[I32 time(Ptr), nullptr];
    let random = Random::init_by_seed(123_U64); // time.to_U64);
    let bits = 8192;   //1024;
    let (random, a) = random.generate_bigint(bits);
    let (random, b) = random.generate_bigint(bits);
    timeit("a*b", |_| a*b);;
    timeit("mul_karatsuba(a,b)", |_| mul_karatsuba(a,b));;
    assert_eq(|_| "eq", a*b, mul_karatsuba(a,b));;
    pure()
);

