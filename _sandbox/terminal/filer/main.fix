module Main;

import Minilib.Monad.State;
import Minilib.Terminal.Ncurses;

import Filer.AppEnv;
import Filer.AppView;
import Filer.Common;

main: IO ();
main = do {
    Ncurses::run $ |app_win| (
        CustomColorPair::init;;
        let app_env = *AppEnv::real_env;
        let app_view = *AppView::make(app_env, app_win);
        AppView::run.eval_state_t(app_view)
    )
}.try(eprintln);
