module Main;
foo: () -> I64;
foo = |_| (
    eval write_string(IO::stdout, "Hello World\n");
    eval write_bytes(IO::stdout, "Please type something and Ctrl-D.\n".get_bytes.pop_back);
    eval read_bytes(IO::stdin);
    123
);
main: IO ();
main = (
    let x: I64 = foo();
    println("x="+x.to_string)
);
