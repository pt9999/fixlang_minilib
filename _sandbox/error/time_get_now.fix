module Main;

import Time;

main: IO ();
main = (
    loop_m(
        0, |i|
        if i >= 20 { break_m $ () };
        let now = *Time::get_now;
        let _ = *println("sec=" + now.@sec.to_string + " nanosec=" + now.@nanosec.to_string);
        eval CALL_C[U32 sleep(U32), 1_U32];
        continue_m $ i + 1
    )
);