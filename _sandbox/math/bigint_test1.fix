test_perf_div: IO ();
test_perf_div = (
    println("=== test_perf_div ===");;
    Iterator::range(0, 10).fold_m(
        (), |_, i|
        let prec10 = (i+1) * 1000;
        let bits = (prec10.to_F64 / 0.3010).to_I64;
        let random = Random::init_by_seed(12345_U64);
        let (random, a) = random.generate_bigint(2*bits);
        let (random, b) = random.generate_bigint(bits);
        timeit("_divmod: prec10="+prec10.to_string, |_|
                    let c = _divmod(a.@nat, b.@nat);      // currently the cost of `_divmod` is O(n^3)
                    ());;
        timeit("_divmod2: prec10="+prec10.to_string, |_|
                    let c = _divmod2(a.@nat, b.@nat);      // goldschmidt is about O(n^2)
                    ());;
        timeit("_divmod3: prec10="+prec10.to_string, |_|
                    let c = _divmod3(a.@nat, b.@nat);      // 
                    ());;
        pure()
    )
);

